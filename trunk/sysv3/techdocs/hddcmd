0x00 hard disk data
	0x30 WRITE SECTORS
	0xC5 WRITE MULTIPLE SECTORS
	0xA0 PACKET (ATAPI??)
		0x00 test unit ready
		0x03 request sense
		0x1b start stop unit
		0xbd mechanism status
		0x5a mode sense
			0x00 current values
				0x01 error recovery
				0x2a cd-rom capabilities & mech. stat.
				0x0d cd-rom
				0x0e cd-rom audio control
				0x3f all
			0x01 changeable values
				0x01 error recovery
				0x0d cd-rom
				0x0e cd-rom audio control
				0x2a cd-rom capabilities & mech. stat.
				0x3f all
			0x02 default values
				0x2a cd-rom capabilities & mech. stat., copied from cur vals
				0x01 error recovery
				0x0d cd-rom
				0x0e cd-rom audio ctrl
				0x3f all
			0x03 saved values not implemented
		0x12 inquiry
		0x25 read cd-rom capacity
		0xbe read cd
		0x43 read toc
		0x28 read (10)
		0xa8 read (12)
		0x2b seek
		0x1e prevent/allow medium removal
		0x42 read sub-channel
		0x51 read disc info
		// --- not implemented ---
		0x55: mode select
		0xa6: load/unload cd
		0x4b: pause/resume
		0x45: play audio
		0x47: play audio msf
		0xbc: play cd
		0xb9: read cd msf
		0x44: read header
		0xba: scan
		0xbb: set cd speed
		0x4e: stop play/scan
		0x46: get configuration
		0x4a: get event status notification
0x01 hard disk write precompensation / error register
0x02 hard disk sector count / interrupt reason
0x03 hard disk sector number
0x04 hard disk cylinder low
0x05 hard disk cylinder high
0x06 hard disk drive and head register
0x07 hard disk command / hard disk status
	0x10 CALIBRATE DRIVE
	0x20 READ SECTORS, with retries
	0x21 READ SECTORS, without retries
	0xC4 READ MULTIPLE SECTOR
	0x30 WRITE SECTORS, with retries
	0xC5 WRITE MULTIPLE SECTORS
	0x90 EXECUTE DEVICE DIAGNOSTIC
	0x91 INITIALIZE DRIVE PARAMETERS
	0xEC IDENTIFY DEVICE
	0xEF SET FEATURES
		0x03 set transfer mode
			0x00 PIO default
			0x01 PIO mode
			0x04 MDMA mode
		--- NOT IMPLEMENTED ---
		0x02 enable
		0x82 disable write cache
		0xAA enable
		0x55 disable look-ahead cache
		0xCC enable
		0x66 disable reverting to power on default
	0x40 READ VERIFY SECTORS
	0xC6 SET MULTIPLE MODE
	--- ATAPI commands ---
	0xA1 IDENTIFY PACKET DEVICE
	0x08 DEVICE RESET (ATAPI)
	0xA0 SEND PACKET (ATAPI)
	0xA2 SERVICE (ATAPI), optional
	0xE0 STANDBY NOW
	0xE1 IDLE IMMEDIATE
	0xE5 CHECK POWER MODE
	0x70 SEEK (CGS)
	0xC8 READ DMA
	0xCA WRITE DMA